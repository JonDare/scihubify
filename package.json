{
  "title": "scihubify",
  "name": "scihubify",
  "version": "0.2.3",
  "description": "Open Links on Sci-Hub, search for selected text on Sci-Hub",
  "main": "index.js",
  "author": "JonDare, based on Rev. Wicks Cherrycoke work",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MIT",
  "keywords": [
    "jetpack"
  ],
  "preferences": [{
    "name": "scihubDomain",
    "title": "Sci-Hub Domain",
    "type": "menulist",
    "value": "sci-hub.ac",
    "options": [
      {"value": "sci-hub.bz",
       "label": "sci-hub.bz"},
      {"value": "sci-hub.cc",
       "label": "sci-hub.cc"},
      {"value": "sci-hub.io",
       "label": "sci-hub.io"},
      {"value": "sci-hub.ac",
       "label": "sci-hub.ac"},
      {"value": "scihub22266oqcxt.onion",
       "label": "scihub2226oqcxt.onion (only for Tor users)"}
    ]
  }, {
    "name": "scihubSecure",
    "title": "Use secure connection",
    "type": "bool",
    "value": true
  }]
}
